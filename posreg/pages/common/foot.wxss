/**foot wxss**/
.flex{display:-webkit-flex;display: flex;width:100%;}
.fix_nav_wp{width:100%;position: fixed;left:0;bottom:0;background:#faf7f7;border-top:1rpx solid #e5e5e5;z-index: 10}
.fix_nav_wp .nav_link{flex:1;}
.fix_nav_wp button{display: flex;justify-content:space-between;align-items: center;padding:0;font-size:22rpx;flex-direction: column;color:#999;position: relative;}
/* 清楚按钮的默认样式  */
.defalut_btn{background: transparent;border:none;overflow: visible;padding-left:0;padding-right:0;margin-left:0;margin-right:0;margin-top:2;border-radius:0;}
.defalut_btn:after{display: none;}
.text-primary{color:#00a2e9;}
.fix_nav_wp .nav_link .txt{height:50rpx;line-height:40rpx;font-size:24rpx;color: #9999a5;}
.plus_ico{height:60rpx;width:60rpx;align-self: center}