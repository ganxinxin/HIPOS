/* pages/shopinfo/shoplist.wxss */
@import "../common/foot.wxss";


  /* 列表 */
.themes-list{ background: #FFF; display: block; margin-bottom: 20px; line-height: 20px; }
.themes-list-box{ display: block; position: relative; padding: 16px 20px; border-bottom: 1px solid #efefef; }
.themes-list-thumbnail{ position: absolute; left: 10px; top: 14px; height: 50px; width: 50px; }
.themes-list-number{ font-size: 14px;border-radius: 4px; border: 1px solid #F2F2F2;}
.themes-list-main{ margin-left: 10px; }
.themes-list-shopdetail{ font-size: 14px; color: #444; height: 30px; line-height: 20px; overflow: hidden; }